@model MCBaker.Models.Images
@{
    ViewBag.Title = "Bread";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    @Styles.Render("~/Content/css/bodyCss.css")
</head>

<h2>Bread</h2>

@foreach (String source in Model.imageList)
{
    <img id="foodImage" src="~/Content/images/@(Model.directory)/@(source)" />
}